{
    "name": "咩咩采集",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Save Picture",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "action": {
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery.min.js",
                "lib/mousetrap.min.js",
                "content/index.js"
            ],
            "css": [
                "content/index.css"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "<all_urls>",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}