{
    "package_version": "2.0.3",
    "rule_version": 20200605,
    "rules": [
        {
            "site": "花瓣网",
            "srcPattern": "//hbimg[.]*/*",
            "replaceRule": "'@'.split('/format/')[0].replace(/_sq235$/, '').replace(/_sq75$/, '').replace(/_fw[\\d]+[w]*$/, '').split('_fw')[0].split('/fw/')[0]"
        },
        {
            "site": "Dribbble",
            "srcPattern": "https?://cdn.dribbble.com/*",
            "replaceRule": "'@'.replace(/_1x/g, '').replace(/_teaser/g, '')"
        },
        {
            "site": "新浪微博",
            "srcPattern": "https?://.*[.]sinaimg[.]cn/.*",
            "replaceRule": "'@'.replace(/[/]small[/]/, '/large/').replace(/[/]square[/]/, '/large/').replace(/[/]thumbnail[/]/, '/large/').replace(/[/]bmiddle[/]/, '/large/').replace(/[/]thumb[\\d]*[/]/, '/large/').replace(/[/]mw[\\d]+[/]/, '/large/')"
        },
        {
            "site": "QQ相册/空间",
            "srcPattern": "https?://.*[.]photo[.]store[.]qq[.]com/.*",
            "replaceRule": "'@'.replace(/[/]m[/]/, '/o/').replace(/&w=[\\d]+&h=[\\d]+/, '')"
        },
        {
            "site": "QQ相册2",
            "srcPattern": "https?://group[.]store[.]qq[.]com/.*",
            "replaceRule": "'@'.replace(/[/]400$/, '/800').replace(/[/]200$/, '/800').replace(/[/]100$/, '/800')"
        },
        {
            "site": "百度图趣",
            "srcPattern": "https?://hiphotos[.]baidu[.]com/.*",
            "replaceRule": "'@'.replace(/[/]abpic[/]/, '/pic/').replace(/[/]pin[/]w=[\\d]+[/].+[/]/, '/pin/pic/item/')"
        },
        {
            "site": "豆瓣相册",
            "srcPattern": "https?://img[\\d]*[.]doubanio[.]com/.*",
            "replaceRule": "'@'.replace(/[/]m[/]/, '/l/').replace(/[/]sqs[/]/, '/l/')"
        },
        {
            "site": "Flickr",
            "srcPattern": ".*[.]staticflickr[.]com.*",
            "replaceRule": "'@'.replace(/_[nms]\\.jpg$/, '_b.jpg')"
        },
        {
            "site": "poco.cn",
            "srcPattern": "http?://img[\\d].*pocoimg*",
            "replaceRule": "'@'.replace(/_\\d{3}.jpg$/, '.jpg')"
        },
        {
            "site": "蘑菇街",
            "srcPattern": "https?://.*[.]mogucdn[.]com/.*.jpg",
            "replaceRule": "'@'.replace(/_[\\d]{3}x[\\d]+.jpg$/, '_468x468.jpg').split('.jpg')[0] + '.jpg'"
        },
        {
            "site": "pinterest",
            "srcPattern": "https?://.*[.]pinimg[.]com/.*.jpg",
            "replaceRule": "'@'.replace(/[0-9]+x/g, '736x')"
        },
        {
            "site": "1688",
            "srcPattern": "https?://cbu01.alicdn.com/img/ibank/.*\\..*x.*\\.jpg",
            "replaceRule": "'@'.replace(/\\.\\d+x.*\\./, '.')"
        },
        {
            "site": "淘宝",
            "srcPattern": ".(?:taobao|tb|ali)cdn*(.+)_\\d+x\\d+.jpg+",
            "replaceRule": "'@'.replace(/_\\d+x\\d+.jpg/, '')"
        },
        {
            "site": "天猫",
            "srcPattern": ".(?:taobao|tb|ali)cdn*(.+)_\\d+x\\d.*",
            "replaceRule": "'@'.replace(/_\\d+x\\d.*.jpg/, '')"
        },
        {
            "site": "Houzz",
            "srcPattern": "https://st[.]hzcdn[.]com/.*",
            "replaceRule": "'@'.replace(/fimgs/,'simgs').replace(/_/,'_14-')"
        },
        {
            "site": "HouseBeautiful",
            "srcPattern": "https://hips[.]hearstapps[.]com/.*",
            "replaceRule": "'@'.replace(/[.]jpg{1,}/,'.jpg').split('&resize=')[0]"
        },
        {
            "site": "Officesnapshots",
            "srcPattern": "https://officesnapshots[.]com/.*",
            "replaceRule": "'@'.replace(/-\\d{3,4}x\\d{3,4}/,'')"
        },
        {
            "site": "Archilovers",
            "srcPattern": "https?://img[.]archilovers[.]com/.*",
            "replaceRule": "'@'.replace(/[a-z]_[0-9][0-9][0-9]_/,'').split('?w=')[0]"
        },
        {
            "site": "AD",
            "srcPattern": "https://media[.]architecturaldigest[.]com/.*",
            "replaceRule": "'@'.replace(/w_\\d+/,'w_5000').replace(/,h_\\d+/,'')"
        },
        {
            "site": "Archdaily 中文版",
            "srcPattern": "http://images[.]adsttc[.]com[.]qtlcn[.]com/.*",
            "replaceRule": "'@'.replace(/thumb_/,'large_').replace('/medium_jpg/', '/large_jpg/').replace('/newsletter/', '/large_jpg/')"
        },
        {
            "site": "ArchDaily 国际版",
            "srcPattern": "https://images[.]adsttc[.]com/.*",
            "replaceRule": "'@'.replace(/slideshow/,'large_jpg').replace(/thumb_jpg/,'large_jpg').replace('/medium_jpg/', '/large_jpg/').replace('/newsletter/', '/large_jpg/')"
        },
        {
            "site": "Dezeen",
            "srcPattern": "https://static[.]dezeen[.]com/.*",
            "replaceRule": "'@'.replace(/slideshow/,'large_jpg').replace(/thumb_jpg/,'large_jpg').replace(/-\\d+x\\d+.jpg/,'.jpg')"
        },
        {
            "site": "Archiproducts",
            "srcPattern": "https://img[.]edilportale[.]com/.*",
            "replaceRule": "'@'.replace(/-thumbs.*.[a-z]_/,'s/').replace(/news.*.[a-z]_/,'news/')"
        },
        {
            "site": "Wordpress 通用",
            "srcPattern": "/wp-content/uploads/.*",
            "replaceRule": "'@'.replace(/-\\d+x\\d+/,'').split('?w=')[0]"
        },
        {
            "site": "Squarespace 通用",
            "srcPattern": "https://static[0-9][.]squarespace[.]com/.*",
            "replaceRule": "'@'.replace(/format=[0-9]{3,4}w/,'format=3000w')"
        },
        {
            "site": "Wixstatic 通用规则",
            "srcPattern": "https?://static[.]wixstatic[.]com/.*",
            "replaceRule": "'@'.replace(/\\/v1\\/fill\\/.*/,'')"
        },
        {
            "site": "站酷",
            "srcPattern": "https://img[.]zcool[.]cn/.*",
            "replaceRule": "'@'.replace(/[.]jpg+[.]jpg/,'.jpg').replace(/@\\d{1,4}w.*/, '')"
        },
        {
            "site": "Medium",
            "srcPattern": "https://cdn-images-[0-9][.]medium[.]com/.*",
            "replaceRule": "'@'.replace(/\\/max\\/\\d{2,4}/, '')"
        },
        {
            "site": "Artstation",
            "srcPattern": "https://cdn.*.artstation.com.*",
            "replaceRule": "'@'.replace(/\\d{14}\\/small_square/, 'large').replace(/\\d{14}\\/smaller_square/, 'large').replace(/\\d{14}\\/micro_square/, 'large').replace(/\\/smaller_square/, '/large')"
        },
        {
            "site": "interiordesign",
            "srcPattern": "https://d4qwptktddc5f[.]cloudfront[.]net/.*",
            "replaceRule": "'@'.replace(/easy_thumbnails\\/thumbs_/,'').replace(/[.]jpg.*/,'.jpg')"
        },
        {
            "site": "即刻",
            "srcPattern": "https://cdn[.]ruguoapp[.]com/.*",
            "replaceRule": "'@'.split('?imageMogr2')[0]"
        }
    ]
}